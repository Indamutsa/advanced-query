<!--?xml version="1.0" encoding="UTF-8"?--> <html> <head></head> <body> <ecore:epackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Journal"> <eannotations source="http://www.eclipse.org/OCL/Import"> <details key="ecore" value="http://www.eclipse.org/emf/2002/Ecore"></details> </eannotations> <eannotations source="http://www.eclipse.org/emf/2002/Ecore"> <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"></details> <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"></details> <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"></details> </eannotations> <eclassifiers xsi:type="ecore:EClass" name="Journal"> <eoperations name="getPapers" ordered="false" upperbound="-1" etype="#//Paper"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="body" value="issues.publie->asSet()"></details> </eannotations> </eoperations> <eoperations name="getIssues"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="body" value="issues->select(c:Issue| c.number > numero) "></details> </eannotations> <eparameters name="numero" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> </eoperations> <eoperations name="getResearchPaper" ordered="false" upperbound="-1" etype="#//Issue"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="body" value="issues->select(c:Issue|c.publie)->select(Paper)->select(typeArticle = TYPE_ARTICLE::Recherche)"></details> </eannotations> <eparameters name="typeArticle" lowerBound="1" eType="#//TYPE_ARTICLE" /> </eoperations> <estructuralfeatures xsi:type="ecore:EReference" name="issues" upperBound="-1" eType="#//Issue" eOpposite="#//Issue/journal" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="IssueNumber" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> <estructuralfeatures xsi:type="ecore:EReference" name="autheur" eType="#//Autheur" eOpposite="#//Autheur/journal" /> </eclassifiers> <eclassifiers xsi:type="ecore:EClass" name="Issue"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore"> <details key="constraints" value="limitedTitle notSubmittedPublished afterPublication"></details> </eannotations> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="limitedTitle" value=" if self.title = null then SpecialIssues = true else SpecialIssues = false endif"></details> <details key="notSubmittedPublished" value=" publie->excludesAll(submit)"></details> <details key="afterPublication" value="publie->forAll(p:Paper | p.reviewer->size()=3)"></details> </eannotations> <eoperations name="pageNumber" lowerbound="1" etype="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="post" value="result= publie->collect((LastPage- firstPage)-3)->sum()"></details> </eannotations> </eoperations> <estructuralfeatures xsi:type="ecore:EReference" name="journal" lowerBound="1" eType="#//Journal" eOpposite="#//Journal/issues" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="number" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> <estructuralfeatures xsi:type="ecore:EReference" name="publie" lowerBound="1" upperBound="-1" eType="#//Paper" containment="true" eOpposite="#//Paper/issueOfPublication" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="pages" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="SpecialIssues" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="title" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" /> <estructuralfeatures xsi:type="ecore:EReference" name="submit" upperBound="-1" eType="#//Paper" eOpposite="#//Paper/issueOfSubmission" /> </eclassifiers> <eclassifiers xsi:type="ecore:EClass" name="Paper"> <eannotations source="http://www.eclipse.org/emf/2002/Ecore"> <details key="constraints" value="authorIsnotReviewer"></details> </eannotations> <eannotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"> <details key="authorIsnotReviewer" value="reviewer->forAll(r:Reviewer | redaction->select(a:Autheur|a.nameAuthor = r.nameReviewer )->size()<1) and reviewer->size()<3"></details> </eannotations> <estructuralfeatures xsi:type="ecore:EReference" name="issueOfPublication" lowerBound="1" eType="#//Issue" eOpposite="#//Issue/publie" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="typeArticle" eType="#//TYPE_ARTICLE" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="firstPage" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> <estructuralfeatures xsi:type="ecore:EAttribute" name="LastPage" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger" /> <estructuralfeatures xsi:type="ecore:EReference" name="redaction" lowerBound="1" upperBound="-1" eType="#//Autheur" eOpposite="#//Autheur/ecrit" /> <estructuralfeatures xsi:type="ecore:EReference" name="revisioner" lowerBound="1" upperBound="3" eType="#//Autheur" eOpposite="#//Autheur/examine" /> <estructuralfeatures xsi:type="ecore:EReference" name="reviewer" upperBound="3" eType="#//Reviewer" eOpposite="#//Reviewer/Review" /> <estructuralfeatures xsi:type="ecore:EReference" name="issueOfSubmission" upperBound="-1" eType="#//Issue" eOpposite="#//Issue/submit" /> </eclassifiers> <eclassifiers xsi:type="ecore:EEnum" name="TYPE_ARTICLE"> <eliterals name="Recherche" /> <eliterals name="Application" value="1" /> </eclassifiers> <eclassifiers xsi:type="ecore:EClass" name="Autheur"> <eannotations source="http://www.obeo.fr/dsl/dnc/archetype"> <details key="archetype" value="Role"></details> </eannotations> <estructuralfeatures xsi:type="ecore:EAttribute" name="nameAuthor" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" /> <estructuralfeatures xsi:type="ecore:EReference" name="ecrit" upperBound="-1" eType="#//Paper" eOpposite="#//Paper/redaction" /> <estructuralfeatures xsi:type="ecore:EReference" name="examine" eType="#//Paper" eOpposite="#//Paper/revisioner" /> <estructuralfeatures xsi:type="ecore:EReference" name="journal" eType="#//Journal" eOpposite="#//Journal/autheur" /> </eclassifiers> <eclassifiers xsi:type="ecore:EClass" name="Reviewer"> <estructuralfeatures xsi:type="ecore:EAttribute" name="nameReviewer" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" /> <estructuralfeatures xsi:type="ecore:EReference" name="Review" upperBound="-1" eType="#//Paper" eOpposite="#//Paper/reviewer" /> </eclassifiers> </ecore:epackage> </body> </html> 